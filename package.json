{
  "name": "@defo550/rgb-to-hex",
  "version": "0.0.0-development",
  "description": "Convert rgb color to hexadecimal.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist --ignore *.spec.js",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "commit": "git-cz",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha src/index.spec.js --compilers js:babel-register",
    "test:watch": "npm t -- -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/defo550/rgb-to-hex.git"
  },
  "keywords": [
    "rgb",
    "rgba",
    "hex",
    "hexadecimal",
    "color",
    "convert"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Christopher DeFelice <defo550@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/defo550/rgb-to-hex/issues"
  },
  "homepage": "https://github.com/defo550/rgb-to-hex#readme",
  "engines": {
    "node": ">= 6.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-register": "6.23.0",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "2.0.0",
    "mocha": "3.2.0",
    "nyc": "10.1.2",
    "rimraf": "2.6.1",
    "semantic-release": "^6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  }
}
